{
  "order": 0,
  "index_patterns": "*customer-data*",
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "mapping": {
        "ignore_malformed": "false"
      }
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "device_ubi_id": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "external_reference": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "name": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "customer_id": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "customer_ref": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "date": {
          "type": "date",
          "format": "date_time_no_millis||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss"
        },
        "device_id": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "device_ref": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "hostname": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "man_id": {
          "type": "integer",
          "store": true,
          "copy_to": "full_fields"
        },
        "customer_name": {
          "type": "keyword",
          "store": true,
          "copy_to": "full_fields"
        },
        "full_fields": {
          "type": "keyword"
        }
         
      }
    }

  },
  "aliases": {}
}
