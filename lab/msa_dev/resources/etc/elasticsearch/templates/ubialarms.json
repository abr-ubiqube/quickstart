{
  "order": 0,
  "index_patterns": "*ubialarm*",
  "settings": {
    "index": {
      "number_of_shards": "1",
      "number_of_replicas": "0",
      "mapping": {
        "ignore_malformed": "false"
      }
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "action": {
          "type": "keyword",
          "store": true
        },
        "app": {
          "type": "keyword",
          "store": true
        },
        "application": {
          "type": "keyword",
          "store": true
        },
        "attack": {
          "type": "keyword",
          "store": true
        },
        "attackid": {
          "type": "keyword",
          "store": true
        },
        "attackname": {
          "type": "keyword",
          "store": true
        },
        "category": {
          "type": "keyword",
          "store": true
        },
        "customer_id": {
          "type": "keyword",
          "store": true
        },
        "customer_name": {
          "type": "keyword",
          "store": true
        },
        "customer_ref": {
          "type": "keyword",
          "store": true
        },
        "date": {
          "type": "date",
          "format": "date_time_no_millis||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSS||epoch_millis||epoch_second"
