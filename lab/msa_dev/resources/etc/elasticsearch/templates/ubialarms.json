          "store": true
        },
        "mod_id": {
          "type": "integer",
          "store": true
        },
        "rawlog": {
          "type": "text",
          "store": true
        },
        "severity": {
          "type": "integer",
          "store": true
        }
      }
    }
  },
  "aliases": {}
}