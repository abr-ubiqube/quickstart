{
        "index_patterns":"*cache*",
        "settings":{
                "index.mapping.ignore_malformed":false,
        "index.number_of_shards": "1",
        "index.number_of_replicas": "0"
        },
        "mappings":{
                "_doc":{
                        "properties": {
                                "date":{
                                        "type":"date",
                                        "store":true,
                                        "format": "date_time_no_millis||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSS||epoch_millis||epoch_second"
                                }
                        }
                }
        }
}